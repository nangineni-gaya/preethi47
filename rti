<body class="roman">
	
		<img src="data:image/jpeg;base64," >

	
	<br>
		<button onclick="myf()">Enter the roman number</button>
	<p id="demo"></p>
	<script>

	function myf(){
        var sum=0;
		var x=prompt("Eneter roman number");
		if(x==null) return -1;

		var p=chartoint(x.charAt(0));

		var prev,cur;
		for(var i=1;i<=x.length;i++){
			cur=chartoint(x.charAt(i));
			prev=chartoint(x.charAt(i-1));
			if(cur<=prev)
				sum=sum+prev;
			else{
                sum=sum-2*prev+cur;
			}
		}
		if(sum>0)
		document.getElementById("demo").innerHTML="Cool! the roman number you have entered is "+sum;
	else
		document.getElementById("demo").innerHTML="Hey! please enter a valid roman number";

 }
 function chartoint(c){
switch (c){
case 'I': return 1;
case 'V': return 5;
case 'X': return 10;
case 'L': return 50;
case 'C': return 100;
case 'D': return 500;
case 'M': return 1000;
default: return -1;
}
}
</script>
<script>
	function goBack(){
		 window.history.back();
	}
</script>
<button onclick=goBack()>Go to the options page</button>
